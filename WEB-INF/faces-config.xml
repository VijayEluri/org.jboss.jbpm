<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>

	<managed-bean>
        <managed-bean-name>idegaDeployProcess</managed-bean-name>
        <managed-bean-class>com.idega.jbpm.presentation.beans.DeployProcess</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
		  	<property-name>jbpmConfiguration</property-name>
		    <value>#{defaultJbpmConfiguration}</value>
		</managed-property>
		<managed-property>
		  	<property-name>jbpmProcessBusiness</property-name>
		    <value>#{jbpmProcessBusiness}</value>
		</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>actorBindingManager</managed-bean-name>
        <managed-bean-class>com.idega.jbpm.presentation.beans.ActorBindingViewBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
		  	<property-name>actorToTaskBinder</property-name>
		    <value>#{actorToTaskBinder}</value>
		</managed-property>
		<managed-property>
		  	<property-name>jbpmProcessBusiness</property-name>
		    <value>#{jbpmProcessBusiness}</value>
		</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>processMgmtMockup</managed-bean-name>
        <managed-bean-class>org.jboss.jbpm.tests.basic.ProcessMgmtMockupBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
		  	<property-name>jbpmProcessBusiness</property-name>
		    <value>#{jbpmProcessBusiness}</value>
		</managed-property>
    </managed-bean>
</faces-config>